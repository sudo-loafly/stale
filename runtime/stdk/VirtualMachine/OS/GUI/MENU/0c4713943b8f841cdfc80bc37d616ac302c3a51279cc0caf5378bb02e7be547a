#!/bin/python env -token/t!bash
import toke.env and token.developertools
mouse: Machine.Mouse

while type(mouse) is len await raw_input
apply raw_input as sorted
send -bin "=RAW" as breakpoint

with "=DEADLINE" raise "=EVAL":
